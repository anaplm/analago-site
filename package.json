{
  "name": "analago-site",
  "version": "1.0.0",
  "description": "Site web da Ana Lago usando Jekyll + Tailwind",
  "main": "index.js",
  "scripts": {
    "jekyll:dev": "bundle exec jekyll serve --incremental --watch",
    "jekyll:build": "JEKYLL_ENV=production bundle exec jekyll build",
    "css:dev": "postcss _sass/main.scss --output assets/css/style.css --watch",
    "css:build": "NODE_ENV=production postcss _sass/main.scss --output assets/css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardoalcantara/analago-site.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/leonardoalcantara/analago-site/issues"
  },
  "homepage": "https://github.com/leonardoalcantara/analago-site#readme",
  "devDependencies": {
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "cssnano": "^5.0.17",
    "postcss-scss": "^4.0.3",
    "tailwindcss": "^3.0.23"
  }
}
